{
    "asi": true,
    "bitwise": true,
    "browser": true,
    "camelcase": true,
    "curly": false,
    "forin": true,
    "immed": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxlen": 100,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "predef": [
        "$",
        "jQuery",
        "JST",
        "App",
        "ActionCable",

        "jasmine",
        "beforeEach",
        "describe",
        "expect",
        "it",

        "angular",
        "inject",
        "module",
        "console",
        "alert"
    ],
    "quotmark": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "esversion": 6
}
