<tr>
  <td><%= icon('far', 'calendar-alt') %> <%= Rent.human_attribute_name(:d_from) %></td>
  <td><%= localized(rent.d_from) %></td>
</tr>
<tr>
  <td><%= icon('far', 'calendar') %> <%= Rent.human_attribute_name(:d_til) %></td>
  <td><%= localized(rent.d_til) %></td>
</tr>
<tr>
  <td><%= icon('fas', 'check') %> <%= Rent.human_attribute_name(:status) %></td>
  <td><%= t(%(model.rent.#{rent.status})) %></td>
</tr>
<% if rent.council.present? %>
  <tr>
    <td><%= icon('fas', 'users') %> <%= Rent.human_attribute_name(:council) %></td>
    <td><%= link_to(rent.council.title, council_path(rent.council)) %></td>
  </tr>
<% end %>
<% if rent.purpose.present? %>
  <tr>
    <td><%= icon('fas', 'exclamation-circle') %> <%= Rent.human_attribute_name(:purpose) %></td>
    <td><%= simple_format(rent.purpose) %></td>
  </tr>
<% end %>
