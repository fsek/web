<%= simple_form_for([:admin, election_document]) do |election_document|%>
    <%= election_document.input :document_name, label: "Namn"%>
    <%= election_document.input :url, label: "Url"%>
    <%= election_document.input :document_type, collection: [["motion", "motion"], ["motionssvar", "motionssvar"]],
        include_blank: false,
        input_html: {class: 'select2'},
        label: "Typ" %>
    <%= election_document.input :reference, collection: @other_documents,
    include_blank: true,
    input_html: {class: 'select2'},
    label: "Associerad med (endast för motionssvar, lämna blank annars)",
    required: false %>
    <%= election_document.button :submit%>
<% end %>