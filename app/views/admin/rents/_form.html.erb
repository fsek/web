<%= simple_form_for([:admin, @rent]) do |f| %>
  <%= f.input :service %>
  <%= f.input :disclaimer %>
  <%= f.input :name %>
  <%= f.input :lastname %>
  <%= f.input :email %>
  <%= f.input :phone %>
  <%= f.input :purpose %>
  <%= f.input :d_from, as: :datetime_picker %>
  <%= f.input :d_til, as: :datetime_picker %>
  <%= f.association :council, collection: Council.titles %>
  <%= f.input :status, collection: [["Ej bestämd"], ["Bekräftad"], ["Nekad"]] %>
  <%= f.input :aktiv %>
  <%= f.input :comment %>
  <%= f.button :submit, class: "btn btn-u", id: 'rent-submit' %>
<% end %>
