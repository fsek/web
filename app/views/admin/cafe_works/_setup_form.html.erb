<%= simple_form_for(@cafe_work, method: :post, url: { action: :setup_create }) do |f| %>
  <%= f.input :lv_first, as: :integer %>
  <%= f.input :lv_last, as: :integer %>
  <%= f.input :lp, as: :integer %>
  <%= f.input :lv, as: :integer %>
  <%= f.input :work_day, as: :datetime_picker %>
  <%= f.button :submit, class: 'btn-u pull-left' %>
  <%= f.button :submit, t(:preview), class: 'btn-u pull-right' %>
<% end %>
