<tr>
  <td><%= link_to document.document_group.name, document.document_group if document.document_group.present? %></td>
  <td><%= link_to document.title, document %> (<%= link_to 'redigera', edit_document_path(document) %>)</td>
  <td><%= document.public ? 'Ja' : 'Nej' %></td>
  <td><%= document.hidden ? 'Ja' : 'Nej' %></td>
  <td><%= document.production_date || '-' %></td>
  <td><%= document.revision_date || '-' %></td>
  <td><%= document.updated_at %></td>
</tr>
