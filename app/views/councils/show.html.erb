<% title(model_name(Council)) %>

<div class="col-md-12">
  <% if @page.present? %>
    <div class="row">
      <%= render @page %>
    </div>
  <% end %>
  <% if @council.posts.present? %>
    <div class="row">
      <%= render 'posts/table', collection: @council.posts %>
    </div>
  <% end %>
</div>
