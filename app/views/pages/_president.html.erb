<%if @council.president.present?%>
	<% post = Post.find(@council.president)%>
<%end%>
<%if(post)%>
	<% user = post.users.first %>
		<%if user%>
			<div class="headline headline-md row">	
				<div class="col-md-9"><br><h4><%=@post.title%> <br><b><%=user%></b></h4></div>
				<%if user.avatar? %>
					<div class="col-md-3"><%= image_tag user.avatar(:thumb), class: "rounded-x"%></div>
				<%end%>
			</div>
		<%end%>
<%end%>	
