<%= simple_form_for(nomination) do |f| %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.association(:post, collection: election.posts.title,
                           include_blank: false,
                           input_html: { class: 'select2' })%>
  <%= f.input :motivation %>
  <%= f.button :submit, id: 'nomination-submit' %>
<% end %>
