<% provide(:title, 'Val') %>
<div class="container val-page">
  <% if @election %>
      <%= render 'index' %>
      <% if (current_user) && (current_user.moderator?(:val)) %>
          <div class="col-md-2 col-md-offset-5"><%= link_to("Redigera valet!", admin_election_path(@election), class: "btn btn-election ") %></div>
      <% end %>
  <% else %>
      <h1>Just nu finns det ingen information om valet.</h1>
      <%= link_to image_tag("layout/f.svg", class: "val-logo"), :root %>
  <% end %>
</div>